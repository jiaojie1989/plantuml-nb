<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Editors">
        <folder name="text">
            <folder name="x-puml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.plantumlnb.ui.Bundle"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.plantumlnb.ui.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
                <folder name="CodeTemplates">
                    <file name="org-netbeans-modules-plantumlnb-editor-snippets.xml" url="org-netbeans-modules-plantumlnb-editor-snippets.xml"/>
                </folder>
            </folder>
        </folder>
    </folder>

    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-puml" url="PUMLSample.puml"/>
            </folder>
        </folder>
    </folder>

</filesystem>
